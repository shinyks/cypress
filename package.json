    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"

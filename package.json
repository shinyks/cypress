    "@babel/runtime": "^7.26.0",
    "@stylistic/eslint-plugin-ts": "^2.10.0",
    "@types/uuid": "^10.0.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
